{
  "name": "kubevious-portable",
  "version": "1.0.1",
  "description": "Kubevious portable backend",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "node index.js",
    "client": "cd ../client && npm start",
    "client-mock": "npm run start-mock --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev-mock": "concurrently \"npm run client-mock\"",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://kubevious@github.com/kubevious/portable.git"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "base-64": "^0.1.0",
    "express": "^4.17.1",
    "flexsearch": "^0.6.32",
    "js-yaml": "^3.13.1",
    "k8s-super-client": "^1.1.14",
    "kubevious-helpers": "git+https://github.com/kubevious/helpers.git",
    "moment": "^2.24.0",
    "morgan": "^1.10.0",
    "the-lodash": "^1.0.15",
    "the-logger": "^1.1.20",
    "the-promise": "^1.1.4",
    "uuid": "^7.0.2",
    "websocket-subscription-server": "^1.0.11",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "mocha": "^6.2.2",
    "should": "^13.2.3"
  },
  "prettier": {
    "tabWidth": 4
  }
}
